_type":"stream","name":"stdout","text":["[0]\n"]}]},{"cell_type":"code","source":["log_odds =logr.coef_\n","odds = numpy.exp(log_odds)\n","print(odds)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"zitRfZbXbhbC","executionInfo":{"status":"ok","timestamp":1742623255801,"user_tz":-330,"elapsed":5,"user":{"displayName":"Gururaj D","userId":"18171965693030485211"}},"outputId":"2258c1bd-e0b9-48fa-b4cd-85ec756ac061"},"execution_count":25,"outputs":[{"output_type":"stream","name":"stdout","text":["[[4.03557295]]\n"]}]},{"cell_type":"code","source":["def logit2prob(logr,x):\n","  log_odds = logr.coef_ * x + logr.intercept_\n","  odds = numpy.exp(log_odds)\n","  probability = odds / (1+odds)\n","  return(probability)\n","\n","print(logit2prob(logr, X))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"2Bxl7qseb4S5","executionInfo":{"status":"ok","timestamp":1742623638922,"user_tz":-330,"elapsed":63,"user":{"displayName":"Gururaj D","userId":"18171965693030485211"}},"outputId":"18fc1f4d-5d8a-48ec-e9d4-222a44c16cba"},"execution_count":29,"outputs":[{"output_type":"stream","name":"stdout","text":["[[0.60749168]\n"," [0.19267555]\n"," [0.12774788]\n"," [0.00955056]\n"," [0.08037781]\n"," [0.0734485 ]\n"," [0.88362857]\n"," [0.77901203]\n"," [0.88924534]\n"," [0.81293431]\n"," [0.57718238]\n"," [0.96664398]]\n"]}]}]}